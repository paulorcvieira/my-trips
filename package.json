    "leaflet": "^1.7.1",
    "next": "10.0.7",
    "next-pwa": "^5.0.5",
    "next-seo": "^4.20.0",
    "nextjs-progressbar": "^0.0.7",
    "polished": "^4.1.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-leaflet": "^3.1.0",
    "styled-components": "^5.2.1",
    "styled-icons": "^10.29.0",
    "webpack": "4"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.13.0",
    "@graphql-codegen/add": "^2.0.2",
