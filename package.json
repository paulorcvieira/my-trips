  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "test": "jest",
    "test:watch": "jest --watch",
    "generate": "yarn plop --plopfile generators/plopfile.js",
